{
    "name": "kanekescom/laravel-helperia",
    "description": "Helper for rapid Laravel development improvements",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Achmad Hadi Kurnia",
            "email": "imachmadhadikurnia@gmail.com",
            "homepage": "https://achmadhadikurnia.com"
        }
    ],
    "homepage": "https://github.com/kanekescom/laravel-helperia",
    "keywords": [
        "laravel",
        "helper",
        "laravel-helperia",
        "helperia"
    ],
    "require": {
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kanekescom\\Helperia\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kanekescom\\Helperia\\Tests\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kanekescom\\Helperia\\HelperiaServiceProvider"
            ],
            "aliases": {}
        },
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}